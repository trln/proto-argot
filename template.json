    // Arrays of ISBNs
    "isbn":{
	"primary":[ //would usually come from 020$a
	    // example
	    {
		"number":"", //numeric portion only, indexed, can be used for Syndetics ID
		"qualifying_info":"" //any other data in the field, if present
		// combined, the above elements create a display ISBN field
	    }
	],
	"other":[ //would probably come from 020$z and/or 776$z, etc. 
	    // example
	    {
		"number":"", //numeric portion only, indexed
		//could be used as Syndetics ID to increase probability of match, but runs risk or false match (sometimes used for incorrect ISBNs printed on an item)
		"qualifying_info":""
		// combined, the above elements create a display ISBN field, which can always be displayed after the primary ISBNS
	    }
	]
    }

    // Array of ISSNs
    "issn":{
	"primary":[], //ee:ISSN
	"linking":[], //ee:Linking ISSN
	"series":[] //ee:Series ISSN
    }
    

    // IDs to use for merging Syndetics data, ee:Syndetics ISBN (can also be UPC for media formats)
